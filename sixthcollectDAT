PREFIX=gattoutput_from_20
SUFFIX=.txt
TIMEDATENOW=$PREFIX$( date +%y_%m_%d_%s)$SUFFIX
echo $TIMEDATENOW
for i in {1..1}
do
	echo "Welcome $i times" 
	sudo /usr/bin/gatttool -b D5:1A:DE:86:93:E9 -t random --char-write-req --handle=0x0025 --value=0100 --listen 2>&1 | tee $TIMEDATENOW
	@sudo /usr/bin/gatttool -b E5:97:EE:07:27:F2 -t random --char-write-req --handle=0x0025 --value=0100 --listen 2>&1 | tee $TIMEDATENOW
done
